
@(t:TareaModel)
<tarea id="@t.id">
	<titulo>
		@t.titulo
	</titulo>
	<descripcion>
		@t.descripcion
	</descripcion>
	<fechaCreacion>
		@t.fechacreacion
	</fechaCreacion>
	<fechaFin>
		@t.fechafin
	</fechaFin>
	@if(!t.usuarios.isEmpty){
	<usuarios>
	@for(u <- t.usuarios){
		<usuario>@u.getUsername</usuario>}
	</usuarios>
	}
	@if(!t.tagsTarea.isEmpty){
	<tags>
	@for(tg <- t.tagsTarea){
		<tag>@tg.tag</tag>}
	</tags>
	}
</tarea>