@(u:UsuarioModel)
<usuario id="@u.id">
	<username>
		@u.username
	</username>
	<nombre>
		@u.nombre
	</nombre>
	<edad>
		@u.edad
	</edad>
	<password>
		@u.pass.getContrasenia
	</password>
	<tareas>
		@for(t <- u.tareasUsuario){<tarea>@t.getTitulo</tarea>}
	</tareas>
	<emails>
		@for(e <- u.emails){<email>@e.getEmail</email>}
	</emails>
</usuario>