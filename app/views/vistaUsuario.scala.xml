@(u:UsuarioModel)
<usuario id="@u.id">
	<username>
		@u.username
	</username>
	<nombre>
		@u.nombre
	</nombre>
	<edad>
		@u.edad
	</edad>
	<telefono>
		@u.telefono
	</telefono>
	<tareas>
		@for(t <- u.tareasUsuario){<tarea>@t.titulo</tarea>}
	</tareas>
	<emails>
		@for(e <- u.emails){<email>@e.email</email>}
	</emails>
</usuario>