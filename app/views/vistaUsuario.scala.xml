@(u:UsuarioModel)
<usuario>
	<id>
		@u.id
	</id>
	<nombre>
		@u.nombre
	</nombre>
	<edad>
		@u.edad
	</edad>
	<password>
		@u.pass.getContrasenia
	</password>
	<tareas>
		@for(t <- u.tareasUsuario){<tarea>@t.texto</tarea>}
	</tareas>
	<emails>
		@for(e <- u.emails){<email>@e.email</email>}
	</emails>
</usuario>